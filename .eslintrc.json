{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "indent": [ "error", 4 ],
      "quotes": [ "error", "double" ],
      "semi": [ "error", "always" ],
      "no-console": "off"
    }
}
